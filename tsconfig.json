{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es2019",
        "lib": [
            "ES2019"
        ],
        "outDir": "out",
        "sourceMap": true,
        "rootDir": "src",
        "strict": true,
        "allowUnreachableCode": false,
        "noImplicitAny": true,
        "noImplicitReturns": true
    },
    "exclude": [
        "node_modules",
        ".vscode-test"
    ]
}